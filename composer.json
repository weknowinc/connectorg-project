{
  "name": "weknowinc/connectorg-project",
  "description": "Project template for connectorg distribution.",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "connectorg",
      "homepage": "https://github.com/weknowinc",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/connectorg",
    "source": "http://cgit.drupalcode.org/connectorg"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "cweagans/composer-patches": "~1.0",
    "drupal/core-composer-scaffold": "^8.8.0",
    "drupal/core-project-message": "^8.8.0",
    "weknow/connectorg": "1.x-dev"
  },
  "config": {
    "process-timeout": 0
  },
  "scripts": {
    "quick-start": [
      "composer install",
      "php web/core/scripts/drupal quick-start connectorg --no-interaction"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "conflict": {
    "drupal/drupal": "*"
  },
  "extra": {
    "drupal-scaffold": {
      "allowed-packages": [
        "drupal/core"
      ],
      "locations": {
        "web-root": "web/"
      }
    },
    "drupal-core-project-message": {
      "include-keys": ["homepage", "support"],
      "post-create-project-cmd-message": [
        "<bg=blue;fg=white>                                                         </>",
        "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the connectorg distribution </>",
        "<bg=blue;fg=white>  from the weknowinc/connectorg-project template!              </>",
        "<bg=blue;fg=white>                                                         </>",
        "",
        "<bg=yellow;fg=black>Next steps</>:",

        "  * Install the site: https://www.drupal.org/docs/8/install",
        "  * Remove the plugin that prints this message:",
        "      composer remove drupal/core-project-message"
      ]
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/profiles/{$name}": ["type:drupal-profile"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    }
  }
}