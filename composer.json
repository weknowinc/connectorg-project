{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "ext-pdo": "*",
        "balbuf/drupal-libraries-installer": "^1.0",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/address": "~1.0",
        "drupal/core": "9.0.0-rc1 as 8.8.x-dev",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/ctools": "^3.2",
        "drupal/pathauto": "^1.6",
        "drupal/search_api": "^1.15",
        "drupal/smart_date": "^2.5",
        "drupal/social_api": "2.x-dev",
        "drupal/social_auth": "2.x-dev",
        "drupal/social_auth_facebook": "2.x-dev",
        "drupal/social_auth_google": "2.x-dev",
        "drupal/token": "^1.6",
        "drupal/twig_tweak": "^2.4",
        "drupal/upgrade_status": "^2.6",
        "drupal/vmi": "^2.5",
        "eluceo/ical": "^0.16.0",
        "symfony/http-foundation": "^4.4.7",
        "vlucas/phpdotenv": "^4.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "drupal-libraries": {},
        "patches": {
            "drupal/core": {
                "3103529 - http installation of 8.8.1 fails": "https://www.drupal.org/files/issues/2020-01-29/3103529-22.patch"
            },
            "drupal/social_auth": {
                "3131765 - Drupal 9 Readiness": "https://www.drupal.org/files/issues/2020-04-29/3131765-2.patch"
            },
            "drupal/social_api": {
                "3134034-3 - Drupal 9 compatibility": "https://www.drupal.org/files/issues/2020-05-08/d9-3134034-3.patch",
                "3134034-4 - Drupal 9 compatibility": "https://www.drupal.org/files/issues/2020-05-22/3134034-4.patch"
            },
            "drupal/social_auth_google": {
                "3119615 - Add the 'core_version_requirement' key in info.yml": "https://www.drupal.org/files/issues/2020-03-17/3119615-2.patch"
            },
            "drupal/social_auth_facebook": {
                "3119616-2 - Add the 'core_version_requirement' key in info.yml": "https://www.drupal.org/files/issues/2020-03-17/3119616-2.patch",
                "3119616-3 - Add the 'core_version_requirement' key in info.yml": "https://www.drupal.org/files/issues/2020-04-14/3119616-3.patch"
            },
            "drupal/smart_date": {
                "3137811 - Drupal 9 compatibility": "https://www.drupal.org/files/issues/2020-05-19/3137811-3.patch"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/sites/all/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
