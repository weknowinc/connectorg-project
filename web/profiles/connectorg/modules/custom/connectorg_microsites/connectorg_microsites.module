<?php

/**
 * @file
 * Contains connectorg_microsites.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function connectorg_microsites_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the connectorg_microsites module.
    case 'help.page.connectorg_microsites':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Module to manage microsites') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function connectorg_microsites_theme() {
  return [
    'connectorg_microsites' => [
      'render element' => 'children',
    ],
  ];
}
