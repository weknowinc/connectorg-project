<?php

/**
 * @file
 * Contains connectorg_tool.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function connectorg_tool_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the connectorg_tool module.
    case 'help.page.connectorg_tool':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Module to handle all information from tools') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function connectorg_tool_theme() {
  return [
    'connectorg_tool' => [
      'render element' => 'children',
    ],
  ];
}
